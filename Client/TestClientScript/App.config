<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--目标服务器地址-->
    <add key="HOST" value="127.0.0.1"/>
    <!--目标服务器端口号-->
    <add key="PORT" value="10022"/>
    <!--缓冲区长度-->
    <add key="BUFSIZE" value="1024"/>
    <!--TCP最多连接数-->
    <!--这边Client不涉及过多的连接数-->
    <!--add key="MAXLISTEN" value="20"/-->
    <!--方法类名和参数之间的分割符-->
    <add key="SPLIT_STR" value="the split str"/>
    <!--尾插法的尾部标识-->
    <add key="TRAILER" value="something useable as an end marker"/>
    <!--两秒-->
    <add key="MAX_BLOCK_TIME" value="2"/>
  </appSettings>
</configuration>